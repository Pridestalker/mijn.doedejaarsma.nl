{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "binarytorch/larecipe": "^2.1",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.2",
        "lab404/laravel-impersonate": "^1.4",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.4",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "nuwave/lighthouse": "dev-master",
        "silber/bouncer": "v1.0.0-rc.5",
        "tucker-eric/eloquentfilter": "^1.4"
    },
    "require-dev": {
        "ajthinking/tinx": "^2.4",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.3",
        "beyondcode/laravel-query-detector": "^1.0",
        "bmitch/churn-php": "^1.0",
        "filp/whoops": "^2.5",
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.8",
        "haydenpierce/class-finder": "^0.3.3",
        "laravel/dusk": "^5.5",
        "mll-lab/laravel-graphql-playground": "^1.1",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.4",
        "phpunit/phpunit": "^8",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "*",
        "sven/artisan-view": "^3.2",
        "vimeo/psalm": "^3.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "psalm": "psalm",
        "churn": "churn run ./app",
        "larastan": "@php artisan code:analyse",
        "phpunit": "phpunit"
    }
}
