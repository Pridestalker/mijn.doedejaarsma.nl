{
  "private": true,
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "cypress": "./node_modules/.bin/cypress open",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.4",
    "@babel/plugin-proposal-class-properties": "^7.4",
    "@babel/plugin-proposal-decorators": "^7.4",
    "axios": "^0.19",
    "babel-jest": "^24.8",
    "bootstrap": "^4.3",
    "browser-sync": "^2.26",
    "browser-sync-webpack-plugin": "2.2",
    "cross-env": "^5.2",
    "date-fns": "^2.0.0-alpha.31",
    "jquery": "^3.4",
    "laravel-mix": "^4.0",
    "lodash": "^4.17",
    "popper.js": "^1.15",
    "resolve-url-loader": "^3.1",
    "sass": "^1.20",
    "sass-loader": "^7.1",
    "ts-loader": "^6.0",
    "typescript": "^3.5",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1",
    "vue-property-decorator": "^8.1",
    "vue-router": "^3.0",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.1",
    "vuex-class-modules": "^1.0"
  }
}
